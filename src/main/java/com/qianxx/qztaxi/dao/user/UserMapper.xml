<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qianxx.qztaxi.dao.user.UserDao">

    <select id="getByMobile" parameterType="map" resultType="com.qianxx.qztaxi.po.User">
        SELECT
        id,
        username,
        password,
        mobile
        FROM user_info
        <where>
            mobile = #{mobile}
        </where>
    </select>

</mapper> 